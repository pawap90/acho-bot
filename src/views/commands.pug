style. 
    body {
        font-family: Arial, Helvetica, sans-serif;
        font-weight: bold;
        font-size: small;
        background-color: #2F364A;
        color: #f5f5f5;
        padding: 20px;
    }
    h1 {
        color: #B468D8;
    }
    table {
        padding-top: 20px;
        border-collapse: collapse;
        font-size: small;
    }
    td {
        padding-left: 20px;
        padding-right: 20px;
        padding-top: 10px;
        padding-bottom: 10px;
        border-bottom: solid 1px #2F364A;
        background-color: #343D56;
    }
    code {
        font-size: medium;
    }
    .eg {
        color: #B468D8;
    }
    .permissions {
        color: #B468D8;
        font-size: x-small;
        padding-left: 10px;
    }

h1 AchoBot Commands

table
    tbody 
        each command in commands
            tr 
                td 
                    code #{command.CommandName}
                    p.permissions #{command.Permissions}
                td 
                    if command.Example && command.Example.length > 0
                        p.eg 
                            code #{command.Example}
                td #{command.Description}
